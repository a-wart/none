<%= flash[:notice] %>
<table>
  <thead>
    <tr>
      <th>Link</th>
      <th>Timer</th>
      <th>Reflink</th>
    </tr>
  </thead>
  <tbody>
    <% @faucets.each do |faucet| %>
      <tr>
        <td><%= faucet.link %></td>
        <td><%= faucet.timer %></td>
        <td><%= faucet.reflink %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<div>
<h3>Import a CSV File</h3>
  <%= form_tag faucets_import_path, multipart: true do %>
    <%= file_field_tag :file %>
    <%= submit_tag "Import CSV" %>
  <% end %>
</div>